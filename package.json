{
  "name": "vocabs-new",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start --dev-client --tunnel -c",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "clearJestCache": "jest --clearCache",
    "testFile": "jest --watch --coverage=false --testPathPattern=src/features/vocabs/vocabList/USE_myVocabs/helpers/USE_myVocabsReducer/Vocab_REDUCER/helpers/ADD_newPaginatedVocabsToReducerState/ADD_newPaginatedVocabsToReducerState.test.ts",
